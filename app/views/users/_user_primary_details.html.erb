<div class="media" style="float:left;width:75%">
	<div id='user_primary_info_<%= user[:id] %>' class="hand_cursor media opacity-05" onclick="show_mesa_user_detail(<%= user[:id] %>);">
						  <div class="media-left media-middle">
						    <img class="media-object search_user_img" src="<%= user[:profile_pic] %>"  alt="..." height="55px" width="55px">
						  </div>
						  <div class="media-body">
						    <div class="media-heading user_name">
						      <span><%= user[:name] %></span>
						      <% if search_type == 'chair' %> 
							<img src="assets/next.png" class="pull-right expandable" alt="select user" style="display:block;" onclick="show_mesa_searched_user_detail(<%= user[:id] %>);" title='Click to see user details'> 
							
						       <% else %>
							<img src="assets/next.png" class="pull-right" alt="select user" style="display:block;">
						       <% end %>
						    </div>
						  </div>
	</div>
</div>
<input type="hidden" name="mesa_type" id="hidden_mesa_type" value="<%= mesa_type %>">


