<script type="text/javascript">
  
   function show_mesa_details(mesa_type,mesa_id){
        
     var url = '/missions/show_'+mesa_type+'_detail';
	    $.ajax({
		url:url,
		type:'get',
		data : {id: mesa_id },
		success:function (data) {
		    {
		    	$("#mesa_col2").html(data);
			//$("#col3").html(data);
   		    }
		}
	    });

       
   }
</script>

